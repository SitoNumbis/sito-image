{
  "name": "sito-image",
  "description": "Image with shimmer loader",
  "main": "dist/App.js",
  "version": "0.10.1",
  "private": false,
  "license": "MIT",
  "author": "SitoNimbus",
  "repository": "https://github.com/SitoNumbis/sito-image.git",
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "publish:npm": "babel src/ -d dist --copy-files",
    "build:min": "BABEL_ENV=minify babel src/ -d dist --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "image",
    "react",
    "shimmer",
    "@emotion"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/preset-react": "^7.16.7",
    "babel-preset-minify": "^0.5.2"
  }
}
